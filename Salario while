#include "stdafx.h"
#include<iostream>
#include"conio.h"

using namespace std;

void main(){
	float sb,sn;
	int n,i=0;

	cout<<"\nIntroduza la cantidad de empleados:";
	cin>> n;
	while(i<n)
	{
		cout<<"Introduzca salario bruto: ";
		cin>>sb;
		if(sb>2960)
			sn=(sb-((sb*0.13)-(sb*0.121)));
		else
			sn=(sb-(sb*0.121));
		cout<<"Salario neto: "<<sn<<endl;
		i++;
	}

	
getch();
}
